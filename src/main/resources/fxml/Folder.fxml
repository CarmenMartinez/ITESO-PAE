<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="196.0" prefWidth="174.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.gui.controllers.FolderController">
  <children>
    <Label layoutX="28.0" layoutY="19.0" text="%folder_name">
      <font>
        <Font size="16.0" />
      </font>
    </Label>
    <TextField fx:id="textFieldFolderName" layoutX="21.0" layoutY="57.0" prefWidth="124.0" />
    <Label layoutX="23.0" layoutY="103.0" text="%folder_favorite" />
    <CheckBox fx:id="checkBoxFavorite" layoutX="128.0" layoutY="102.0" mnemonicParsing="false" text="" />
    <Button layoutX="24.0" layoutY="155.0" mnemonicParsing="false" onAction="#accept" text="%folder_accept" />
    <Button layoutX="96.0" layoutY="155.0" mnemonicParsing="false" onAction="#cancel" text="%folder_cancel" />
  </children>
</AnchorPane>
