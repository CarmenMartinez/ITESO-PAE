<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.CreateAccountController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0">
         <children>
            <Pane layoutY="-1.0" prefHeight="355.0" prefWidth="230.0">
               <children>
                  <TextField id="firstName" fx:id="textFieldFirstName" layoutX="26.0" layoutY="74.0" />
                  <Label id="app-name" alignment="CENTER" layoutX="67.0" layoutY="16.0" text="TasksFx" />
                  <TextField id="lastName" fx:id="textFieldLastName" layoutX="26.0" layoutY="134.0" />
                  <Button id="accept" contentDisplay="CENTER" layoutX="26.0" layoutY="302.0" mnemonicParsing="false" onAction="#createAccount" prefHeight="26.0" prefWidth="170.0" text="%create_accept" />
                  <Label contentDisplay="CENTER" layoutX="27.0" layoutY="51.0" text="%create_first_name" />
                  <Label contentDisplay="CENTER" layoutX="25.0" layoutY="112.0" text="%create_last_name" />
                  <Label contentDisplay="CENTER" layoutX="25.0" layoutY="171.0" text="%create_email" />
                  <TextField id="email" fx:id="textFieldEmail" layoutX="26.0" layoutY="193.0" />
                  <Label contentDisplay="CENTER" layoutX="24.0" layoutY="233.0" text="%create_password" />
                  <TextField id="password" fx:id="textFieldPassword" layoutX="26.0" layoutY="256.0" />
               </children>
            </Pane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
